{
  "name": "@dapp-sh/icon",
  "version": "0.1.0",
  "description": "A agnostic web component ready use icons from iconify",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": ["./dist/*", "./dist/index.d.ts"]
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "unbuild",
    "fmt": "biome format --write .",
    "clean": "rimraf node_modules pnpm-lock.yaml",
    "lint": "biome lint --write ."
  },
  "keywords": [],
  "author": "sebastiandotdev",
  "license": "ISC",
  "packageManager": "pnpm@9.10.0",
  "devDependencies": {
    "@biomejs/biome": "1.9.2",
    "@types/node": "22.7.4",
    "rimraf": "^6.0.1",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2",
    "unbuild": "^2.0.0"
  },
  "dependencies": {
    "@iconify/collections": "^1.0.465"
  }
}
